{
  "name": "@uniswap/v3-sdk-next",
  "version": "1.0.0",
  "description": "Modern SDK for building applications on top of Uniswap V3 with native BigInt and ox",
  "repository": "https://github.com/Uniswap/sdks.git",
  "keywords": ["uniswap", "ethereum", "bigint", "ox", "v3"],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@uniswap/sdk-core-next": "file:../sdk-core",
    "@uniswap/swap-router-contracts": "^1.3.1",
    "@uniswap/v2-sdk-next": "file:../v2-sdk",
    "@uniswap/v3-periphery": "^1.4.4",
    "@uniswap/v3-staker": "^1.0.2",
    "ox": "^0.6.0",
    "tiny-invariant": "^1.3.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
