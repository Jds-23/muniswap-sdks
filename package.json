{
  "name": "muniswap-sdks",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "docs:dev": "turbo run dev --filter=@muniswap/docs",
    "docs:build": "turbo run build --filter=@muniswap/docs"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  }
}
