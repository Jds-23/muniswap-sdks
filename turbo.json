{
  "pipeline": {
    "lint": {
      "inputs": [
        "sdks/*/src/**.ts",
        "sdks/*/src/**.tsx"
      ]
    },
    "build": {
      "inputs": [
        "yarn.lock",
        "**/package.json",
        "sdks/*/src/**.ts",
        "sdks/*/src/**.tsx"
      ],
      "outputs": [
        "sdks/*/dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "sdks/*/dist/**"
      ]
    },
    "release": {
      "dependsOn": []
    },
    "typecheck": {
      "dependsOn": []
    },
    "docs:dev": {
      "cache": false,
      "persistent": true
    },
    "docs:build": {
      "inputs": [
        "sdks/new/docs/**"
      ],
      "outputs": [
        "sdks/new/docs/dist/**"
      ]
    },
    "docs:preview": {
      "dependsOn": [
        "docs:build"
      ],
      "cache": false
    }
  }
}
